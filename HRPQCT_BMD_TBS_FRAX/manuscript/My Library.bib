@article{schiniOverviewUseFracture2024,
  title = {An Overview of the Use of the Fracture Risk Assessment Tool ({{FRAX}}) in Osteoporosis},
  author = {Schini, M. and Johansson, H. and Harvey, N. C. and Lorentzon, M. and Kanis, J. A. and McCloskey, E. V.},
