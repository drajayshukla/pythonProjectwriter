@article{p.geusensOp0080EffectDenosumab2019,
  title = {Op0080\>Effect of Denosumab versus Risedronate on Cortical and Trabecular Bone Microarchitecture by High Resolution Peripheral Quantitative Computed Tomography (Hr-Pqct) in Glucocorticoid-Treated Individuals},
  author = {P. Geusens, S. Goemaere, N. Pannacciulli},
